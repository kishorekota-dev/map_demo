# DialogFlow Configuration
GOOGLE_PROJECT_ID=your-dialogflow-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# MCP Server Configuration
MCP_SERVER_URL=http://localhost:3001
MCP_SERVER_API_KEY=your-mcp-server-api-key

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret

# Enterprise Banking API Configuration
BANKING_API_URL=http://localhost:8080
BANKING_API_VERSION=v1

# Application Configuration
NODE_ENV=development
DEBUG_MODE=true

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key
JWT_SECRET=your-jwt-secret-key

# DialogFlow Settings
DIALOGFLOW_LANGUAGE_CODE=en-US
DIALOGFLOW_SESSION_TIMEOUT=3600

# LangChain Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langchain-api-key
LANGCHAIN_PROJECT=chatbot-banking

# Redis Configuration (for session storage)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
